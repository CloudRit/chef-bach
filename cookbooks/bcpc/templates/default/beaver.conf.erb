[beaver]
rabbitmq_host: <%="#{node[:bcpc][:management][:vip]}"%>
rabbitmq_password: <%="#{get_config('rabbitmq-password')}"%>
rabbitmq_exchange_durable: 1
rabbitmq_queue_durable: 1
rabbitmq_ha_queue: 1

[/var/log/{syslog,auth.log}]
type: syslog

[/var/log/apache2/access.log]
type: apache-access

[/var/log/apache2/error.log]
type: apache-error

[/var/log/ceph/ceph-*.log]
type: ceph-daemons

[/var/log/ceph/ceph.log]
type: ceph-activity

[/var/log/cinder/*.log]
type: cinder

[/var/log/glance/*.log]
type: glance

[/var/log/keystone/*.log]
type: keystone

[/var/log/nova/*.log]
type: nova

[/var/lib/mysql/<%="#{node.hostname}"%>.err]
type: mysql

[/var/log/rabbitmq/rabbit@<%="#{node.hostname}"%>.log]
type: rabbitmq

[/var/log/elasticsearch/<%="#{node[:bcpc][:region_name]}"%>.log]
type: elasticsearch

[/var/log/diamond/diamond.log]
type: diamond

[/var/log/dmesg]
type: dmesg

[/var/log/dpkg.log]
type: dpkg

[/var/log/kern.log]
type: kern
